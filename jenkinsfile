pipeline{
agent none
  stages{
  stage ('all') {
  parallel {
      stage ('BUILD') {
        agent { label 'lab1' }
        steps {
           sh '''
           echo "this is to test build stage"
           '''
        }
      }
      stage ('DEPLOY') {
        agent { label 'lab2' }
        steps {
           echo "this is to test deploy stage"
        }
      }
      stage ('TESTING1') {
        agent any
        steps {
           sh 'echo "this is to test testing stage"'
        }
      }
      stage ('TESTING2') {
        agent { label 'master' }
        steps {
           echo "this is to test testing2 stage"
        }
      }
      }
}       
}
