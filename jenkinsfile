    stages{
      stage ('BUILD') {
        steps {
           sh '''
           echo "this is to test build stage"
           '''
        }
      }
      stage ('DEPLOY') {
        steps {
           echo "this is to test deploy stage"
        }
      }
      stage ('TESTING') {
        steps {
           sh 'echo "this is to test testing stage"'
        }
      }
      stage ('TESTING2') {
        steps {
           echo "this is to test testing2 stage"
        }
      }
    }
}       
